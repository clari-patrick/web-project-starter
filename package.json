{
  "name": "web-project-starter",
  "version": "1.0.0",
  "private": true,
  "author": "Patrick Smith",
  "license": "See license in https://www.creative-tim.com/license",
  "description": "React web project started kit with enterprise grade dev tooling including typescript, code generation templates, and testing by Patrick Smith",
  "homepage": "https://demos.creative-tim.com/material-kit-react/#/",
  "bugs": {
    "url": "https://github.com/creativetimofficial/ct-material-kit-react/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/creativetimofficial/ct-material-kit-react.git"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "dependencies": {
    "@emotion/cache": "11.10.3",
    "@emotion/react": "11.10.4",
    "@emotion/styled": "11.10.4",
    "@mui/icons-material": "5.10.6",
    "@mui/material": "5.10.8",
    "@mui/styled-engine": "5.10.8",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "chroma-js": "2.4.2",
    "flatpickr": "4.6.13",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-copy-to-clipboard": "5.1.0",
    "react-countup": "6.3.1",
    "react-dom": "18.2.0",
    "react-flatpickr": "3.10.13",
    "react-router-dom": "6.4.1",
    "react-scripts": "5.0.1",
    "react-syntax-highlighter": "15.5.0",
    "web-vitals": "3.0.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src --color",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx src --color",
    "gen": "npx generate-react-cli component",
    "gen:page": "npx generate-react-cli component --type=page"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint": "8.24.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.8",
    "eslint-plugin-react-hooks": "4.6.0",
    "generate-react-cli": "^7.4.0",
    "prettier": "2.7.1",
    "react-test-renderer": "^18.2.0"
  }
}
